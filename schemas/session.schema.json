{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "sessionDatetime": {
            "type": "string",
            "format": "date-time"
        },
        "sessionName": {
            "type": "string"
        },
        "sessionType": {
            "type": "string",
            "enum": [
                "Practice",
                "Qualifying",
                "Race",
                "Testing",
                "Other"
            ]
        }
    },
    "required": [
        "sessionDatetime",
        "sessionName",
        "sessionType"
    ]
}